<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <Version>0.9.1</Version>
    <Authors>Jan Kruse</Authors>
    <PackageProjectUrl>https://github.com/Jan-Kruse/DotCAML</PackageProjectUrl>
    <RepositoryUrl>https://github.com/Jan-Kruse/DotCAML</RepositoryUrl>
    <GeneratePackageOnBuild>false</GeneratePackageOnBuild>
    <PackageRequireLicenseAcceptance>false</PackageRequireLicenseAcceptance>
    <PackageTags>caml builder sharepoint</PackageTags>
    <RepositoryType>git</RepositoryType>
    <NeutralLanguage>en-US</NeutralLanguage>
    <Title>DotCAML</Title>
    <PackageId>DotCAML</PackageId>
    <PackageLicenseExpression>MS-PL</PackageLicenseExpression>
    <PublishRepositoryUrl>true</PublishRepositoryUrl>
    <RootNamespace>DotCAML</RootNamespace>
    <AssemblyName>CAML</AssemblyName>
    <Description>DotCAML is a .NET CAML builder based on camljs by Andrei Markeev.</Description>
    <Copyright>Jan Kruse 2019</Copyright>
  </PropertyGroup>
  
</Project>
